extends ../fiddle-view

append vars
  - var type = 'Check'
  - var name = 'headers-visible-text'
  - var relPath = '../'

block fixture

  h3 Pass
  table
    thead
      tr
        th(scope="col") First
        th(scope="col") Last
    tbody
      tr
        td Jerry
        td Seinfeld

  h3 Fail
  table
    thead
      tr
        th(scope="col") First
        th(scope="col" style="display:none;") Last
    tbody
      tr
        td Jerry
        td Seinfeld
